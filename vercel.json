{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "routes": [
        {
            "src": "/editor",
            "dest": "/editor.html"
        },
        {
            "src": "/docs",
            "dest": "/pages/docs.html"
        },
        {
            "src": "/tutorials",
            "dest": "/pages/tutorials.html"
        },
        {
            "src": "/blog",
            "dest": "/pages/blog.html"
        },
        {
            "src": "/changelog",
            "dest": "/pages/changelog.html"
        },
        {
            "src": "/about",
            "dest": "/pages/about.html"
        },
        {
            "src": "/careers",
            "dest": "/pages/careers.html"
        },
        {
            "src": "/contribute",
            "dest": "/pages/contribute.html"
        },
        {
            "src": "/privacy",
            "dest": "/pages/privacy.html"
        },
        {
            "src": "/terms",
            "dest": "/pages/terms.html"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}